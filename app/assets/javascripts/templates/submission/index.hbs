<div class="row">
    <div class="large-12 columns">
        <h1>Submission Details</h1>
    </div>
</div>
<div class="row display">
    <div class="large-2 columns">
        <strong>Language:</strong>
    </div>
    <div class="large-10 columns">
        {{language.name}}
    </div>
</div>
<div class="row display">
    <div class="large-2 columns">
        <strong>Candidate Level:</strong>
    </div>
    <div class="large-10 columns">
        {{candidate.level.text}}
    </div>
</div>
<div class="row display">
    <div class="large-2 columns">
        <strong>File:</strong>
    </div>
    <div class="large-10 columns">
        <a class="button small radius success" {{bind-attr href=zipfile}}><i class="fi-download"></i>&nbsp;Download</a>
    </div>
</div>
<div class="row display">
    <div class="large-2 columns">
        <strong>Email Text:</strong>
    </div>
    <div class="large-10 columns">
        {{emailText}}
    </div>
</div>
